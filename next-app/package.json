{
  "name": "basic-workout-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check-types": "tsc --noemit",
    "lint": "npm run check-types && next lint",
    "test": "jest",
    "todo": "leasot '**/*.{js,jsx,ts,tsx}'",
    "todo-ci": "leasot -x --reporter markdown '**/*.{js,jsx,ts,tsx}' --ignore './db-data','./node_modules' > TODO.md",
    "seed": "npx prisma db seed --preview-feature",
    "reset-seed": "npx prisma migrate reset",
    "start:prod": "npx prisma migrate deploy && npx prisma db seed && node server.js",
    "find:unused": "next-unused"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "next-unused": {
    "alias": {},
    "include": [
      "components",
      "lib"
    ],
    "exclude": [],
    "entrypoints": [
      "app"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.8",
    "@heroicons/react": "^2.0.14",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@next/font": "13.1.5",
    "@prisma/client": "^4.9.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "bcrypt": "^5.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "eslint": "8.32.0",
    "eslint-config-next": "^13.4.16",
    "hashids": "^2.3.0",
    "lucide-react": "^0.263.1",
    "next": "^13.4.16",
    "next-auth": "^4.19.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-toastify": "^9.1.3",
    "sharp": "^0.32.3",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.6",
    "ua-parser-js": "^1.0.35",
    "vaul": "^0.2.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.14.0",
    "autoprefixer": "^10.4.13",
    "jest": "^29.5.0",
    "leasot": "^13.3.0",
    "postcss": "^8.4.21",
    "prisma": "^4.9.0",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
