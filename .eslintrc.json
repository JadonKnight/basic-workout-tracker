{
  "extends": "next/core-web-vitals",
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "eol-last": [
      "error",
      "always"
    ],
    "no-trailing-spaces": [
      "error"
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 1
      }
    ],
    "arrow-parens": [
      "error",
      "always"
    ],
    "indent": [
      "error",
      2
    ],
    "no-unused-expressions": [
      "error"
    ],
    "no-unused-vars": [
      "error"
    ]
  }
}
